{
  "name": "romanzaycev/vibeduck",
  "description": "Open-source vibecoding tool on PHP, powered by LLMs",
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Romanzaycev\\Vibe\\": "src/"
    }
  },
  "require": {
    "php": ">=8.3",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-libxml": "*",
    "ext-dom": "*",
    "ext-readline": "*",
    "symfony/console": "^7.3",
    "php-di/php-di": "^7.0",
    "openai-php/client": "^0.13.0",
    "guzzlehttp/guzzle": "^7.9",
    "http-interop/http-factory-guzzle": "^1.2",
    "php-http/guzzle7-adapter": "^1.1",
    "symfony/event-dispatcher": "^7.3",
    "league/commonmark": "^2.7",
    "symfony/finder": "^7.3"
  },
  "authors": [
    {
      "name": "Roman Zaycev",
      "email": "box@romanzaycev.ru"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "php-http/discovery": false
    }
  }
}
